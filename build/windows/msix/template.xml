<?xml version="1.0" encoding="utf-8"?>
<MsixPackagingToolTemplate
    xmlns="http://schemas.microsoft.com/msix/packaging/msixpackagingtool/template/2022">
    <Settings
        AllowTelemetry="false"
        ApplyACLsToPackageFiles="true"
        GenerateCommandLineFile="true"
        AllowPromptForPassword="false">
    </Settings>
    <Installer
        Path="beamdrop-desktop"
        Arguments=""
        InstallLocation="C:\Program Files\ekilie\BeamDrop">
    </Installer>
    <PackageInformation
        PackageName="BeamDrop"
        PackageDisplayName="BeamDrop"
        PublisherName="CN=ekilie"
        PublisherDisplayName="ekilie"
        Version="0.1.0.0"
        PackageDescription="BeamDrop Description">
        <Capabilities>
            <Capability Name="runFullTrust" />
            
        </Capabilities>
        <Applications>
            <Application
                Id="com.wails.beamdrop-desktop"
                Description="BeamDrop Description"
                DisplayName="BeamDrop"
                ExecutableName="beamdrop-desktop"
                EntryPoint="Windows.FullTrustApplication">
                
            </Application>
        </Applications>
        <Resources>
            <Resource Language="en-us" />
        </Resources>
        <Dependencies>
            <TargetDeviceFamily Name="Windows.Desktop" MinVersion="10.0.17763.0" MaxVersionTested="10.0.19041.0" />
        </Dependencies>
        <Properties>
            <Framework>false</Framework>
            <DisplayName>BeamDrop</DisplayName>
            <PublisherDisplayName>ekilie</PublisherDisplayName>
            <Description>BeamDrop Description</Description>
            <Logo>Assets\AppIcon.png</Logo>
        </Properties>
    </PackageInformation>
    <SaveLocation PackagePath="beamdrop-desktop.msix" />
    <PackageIntegrity>
        <CertificatePath></CertificatePath>
    </PackageIntegrity>
</MsixPackagingToolTemplate>
